<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>

  <!-- Servlets -->
  <servlet>
    <servlet-name>phoneBillServlet</servlet-name>
    <servlet-class>edu.pdx.cs410J.sbraich.server.PhoneBillServiceImpl</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>phoneBillServlet</servlet-name>
    <url-pattern>/phonebill/phoneBill</url-pattern>
  </servlet-mapping>

  <!-- Default page to serve -->
  <welcome-file-list>
    <welcome-file>PhoneBillGwt.html</welcome-file>
  </welcome-file-list>
</web-app>
